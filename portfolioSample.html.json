slate(7px, -4px);\n}\n\n.hamburger-menu_1656760375322_497.open .hamburger-btn span:nth-last-child(2) {\n    opacity: 0;\n}\n\n.hamburger-menu_1656760375322_497 .hamburger-btn span:nth-child(3) {\n    transform-origin: -11px 4px;\n}\n.hamburger-menu_1656760375322_497.open .hamburger-btn span:nth-child(3) {\n    transform: rotate(-45deg) translate(0, 12px);\n}\n\n/* themes */\n\n.hamburger-menu_1656760375322_497 ul li {\n  padding: 20px 4px;\n  border-bottom: 1px solid #b3b3b3;\n  list-style-type: none;\n  color: #4B4B4B;\n  margin: 0 60px 0 20px;\n}\n\n.hamburger-menu_1656760375322_497 ul li a {\n  color: #4B4B4B;\n}\n\n.hamburger-menu_1656760375322_497.open nav {\n  top: -20px;\n}\n\n.hamburger-menu_1656760375322_497 nav {\n  width: 101vw !important;\n  margin: 0 -1px;\n}\n\n\n</style>\n<!-- inspired by https://codepen.io/erikterwan/pen/EVzeRP -->\n<div class=\"hamburger-menu_1656760375322_497 closed\">\n    <div class=\"hamburger-btn fixed\">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav class=\"\">\n            <ul>\n            \n                <li><a\n                    \n                        href=\"#!page-projects\"\n                    \n                        linkType=\"LinkTypePage\"\n                    \n                        title=\"Projects\"\n                    \n                ></a></li>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    document.querySelector('.hamburger-menu_1656760375322_497').classList.remove('open');\n    document.querySelector('.hamburger-menu_1656760375322_497').classList.add('closed');\n  }\n  function open() {\n    isOpen = true;\n    document.querySelector('.hamburger-menu_1656760375322_497').classList.add('open');\n    document.querySelector('.hamburger-menu_1656760375322_497').classList.remove('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  function scrollOrResize() {\n    close();\n    move();\n  }\n  var silexElement = document.querySelector('.hamburger-menu_1656760375322_497').closest('.editable-style');\n  function move() {\n    var data = window.silex\n    var rect = silexElement.getBoundingClientRect();\n    var offset = {\n      top: rect.top + data.scrollTop,\n      left: rect.left + data.scrollLeft,\n    };\n    var container = document.querySelector('.hamburger-menu_1656760375322_497 .nav-container');\n    container.style.transform = 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)';\n  }\n  document.querySelector('.hamburger-menu_1656760375322_497 .hamburger-btn').addEventListener('click', function() { toggle(); });\n  document.addEventListener('resize', scrollOrResize);\n  document.addEventListener('scroll', scrollOrResize);\n  scrollOrResize()\n  setTimeout(() => scrollOrResize(), 0)\n})();\n</script>\n"},{"id":"silex-id-1656761258840-4","tagName":"DIV","type":"text-element","alt":null,"title":null,"isSectionContent":false,"visibility":{"desktop":true,"mobile":true},"style":{"mobile":{},"desktop":{"width":"206px","height":"120px","position":"absolute","top":"511px","left":"146px","padding-left":"0px","padding-right":"0px","padding-bottom":"0px","padding-top":"0px","margin-left":"0px","margin-right":"0px","margin-bottom":"0px","margin-top":"0px"}},"data":{"component":null},"children":[],"pageNames":["page-projects"],"classList":[],"attr":{},"link":{"href":"#!page-projects","linkType":"LinkTypePage","title":"Projects"},"enableEdit":true,"enableDrag":true,"enableDrop":false,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"innerHtml":"<h3>Projects</h3>"}]}